{
  "expo": {
    "name": "PairPay",
    "slug": "green-red",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alisinayousofi.greenred"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alisinayousofi.greenred",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5fd3d24a-fc71-4427-b0d3-dd20f4b537b8"
      }
    },
    "plugins": [
      "expo-localization"
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-1665900038997295~6511893217"
  }
}
